  "scripts": {
    "start": "NODE_ENV=dev concurrently \"react-scripts start\" \"nodemon server/server.js\"",
    "build": "cd server && rm -rf build && cd .. && react-scripts build && mv build server",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "eject": "react-scripts eject",
    "automation-test": "./node_modules/.bin/cucumber-js",
    "start-server": "nodemon server/server.js",
    "ci": "NODE_ENV=test start-server-and-test start-server http-get://localhost:3001"
  },
  "nyc": {
    "all": true,
    "include": [
      "server/**"
    ],
    "check-coverage": false,
    "branches": 50,
    "lines": 50,
    "functions": 50,
    "statements": 50
  },
